      <% bill.items.forEach((item) => { %>
      <ul>  
      <% if (item.username === participant) { %>
        <li>
          <span><%= item.name %></span>
          <span class="item-price"><%= item.price %></span><span>â‚¬</span>
        </li>
      <% } %>
      </ul>
      <% })%>
    </div>
  <% }); %>